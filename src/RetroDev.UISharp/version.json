{
  "version": "0.1",
  "assemblyVersion": "0.1",
  "publicReleaseRefSpec": [ "refs/heads/main" ],
  "nuGetPackageVersion": "$(Version)",
  "informationalVersion": "$(Version)-$(CommitId)",

  "branches": {
    "main": {
      "version": "0.1",
      "increment": "Patch",
      "informationalVersion": "$(Version)"
    },
    "*": {
      "increment": "Patch",
      "informationalVersion": "$(Version)-$(CommitId)"
    }
  },

  "tagPrefix": "v"
}
